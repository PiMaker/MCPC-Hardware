;autotest reg=0 val=6;

global text = "Hello, world!";

func var main(argc, argp) {
    
    // Find index of space
    return indexOf(text, ' ');

}

func var indexOf(haystack, needle) {
    var i = 0;
    while [1] {
        var hsi = [haystack + i];
        var charAt = [$(hsi)];
        if [charAt == needle | charAt == 0] {
            return i;
        }

        i += 1;
    }

    return [-1];
}
